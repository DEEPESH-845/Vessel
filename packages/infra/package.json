{
  "name": "infra",
  "version": "0.1.0",
  "bin": {
    "infra": "bin/infra.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "deploy": "cdk deploy --all --require-approval never",
    "deploy:preview": "cdk deploy --all --require-approval never --no-rollback",
    "destroy": "cdk destroy --all --require-approval never",
    "bootstrap": "cdk bootstrap",
    "list": "cdk list",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf cdk.out dist lib/*.d.ts lib/*.js bin/*.d.ts bin/*.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.1",
    "aws-cdk": "2.1106.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "@nomicfoundation/hardhat-toolbox": "^1.0.2",
    "aws-cdk-lib": "^2.239.0",
    "constructs": "^10.5.1",
    "eslint": "^10.0.1",
    "eslint-config-next": "0.2.4",
    "hardhat": "3.1.9"
  }
}
